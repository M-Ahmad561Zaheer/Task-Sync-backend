{
  "version": 2,
  "rewrites": [
    { "source": "/(.*)", "destination": "/server.js" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "http://localhost:5000" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,PUT,DELETE,OPTIONS" }
        
      ]
    }
  ]
}
